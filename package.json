  "bugs": {
    "url": "https://github.com/oakinogundeji/textpedia/issues"
  },
  "homepage": "https://github.com/oakinogundeji/textpedia#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "mocha": "^3.0.2",
    "partialify": "^3.1.6",
    "should": "^11.0.0",
    "supertest": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "clarify": "^2.0.0",
    "compression": "^1.6.2",
    "ejs": "^2.5.1",
