{
  "name": "textpedia",
  "version": "1.0.0",
  "description": "An app that provides reports to users which serves as preliminary research on keywords passed to the app via SMS",
  "main": "server.js",
  "scripts": {
    "test": "mocha test",
    "start": "nf start",
    "start-dev": "nf run nodemon server.js",
    "build": "browserify -t partialify ./public/js/app.js -o ./public/js/main.js",
